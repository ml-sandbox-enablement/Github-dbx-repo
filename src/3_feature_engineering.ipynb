"source": [
"import sys\n",
"import subprocess\n",
"\n",
"# Install databricks-feature-engineering\n",
"subprocess.check_call([sys.executable, "-m", "pip", "install", "databricks-feature-engineering"])\n",
"\n",
"# Restart Python (this clears variables; keep this as the first cell)\n",
"dbutils.library.restartPython()\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "load-data-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"# Load data from Delta table\n",
"wine_df = spark.table("jpmc_group_catalog.mlops.wine_data_poc_dabs")\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "feature-eng-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"from pyspark.sql.functions import col, log, when\n",
"from pyspark.sql.types import DoubleType\n",
"\n",
"# Log-transform skewed features\n",
"wine_df = wine_df.withColumn("log_residual_sugar", log(col("residual_sugar") + 1))\n",
"wine_df = wine_df.withColumn("log_chlorides", log(col("chlorides") + 1))\n",
"\n",
"# Interaction features\n",
"wine_df = wine_df.withColumn("sulphates_alcohol", col("sulphates") * col("alcohol"))\n",
"wine_df = wine_df.withColumn("acidity_ratio", col("fixed_acidity") / (col("volatile_acidity") + 1e-5))\n",
"\n",
"# Bin pH values: low (<3), medium (3-3.5), high (>3.5)\n",
"wine_df = wine_df.withColumn(\n",
"    "PH_bin",\n",
"    when(col("PH") < 3, "low")\n",
"    .when(col("PH") < 3.5, "medium")\n",
"    .otherwise("high")\n",
")\n",
"\n",
"# One-hot encode PH_bin\n",
"wine_df = wine_df.withColumn("PH_bin_low", (col("PH_bin") == "low").cast(DoubleType()))\n",
"wine_df = wine_df.withColumn("PH_bin_medium", (col("PH_bin") == "medium").cast(DoubleType()))\n",
"wine_df = wine_df.withColumn("PH_bin_high", (col("PH_bin") == "high").cast(DoubleType()))\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "uc-udf-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"# Create a reusable Python UDF in Unity Catalog\n",
"avg_acidity_udf_fn_dabs = """\n",
"CREATE OR REPLACE FUNCTION jpmc_group_catalog.mlops.avg_acidity_udf_fn_dabs(fixed_acidity float, volatile_acidity float, citric_acid float)\n",
"RETURNS float\n",
"LANGUAGE PYTHON\n",
"COMMENT "Extract AVG values of acidity columns"\n",
"AS $$\n",
"def avg_acidity_udf_fn_dabs(fixed_acidity: float, volatile_acidity: float, citric_acid: float) -> float:\n",
"    avg = (fixed_acidity + volatile_acidity + citric_acid) / 3\n",
"    return avg\n",
"return avg_acidity_udf_fn_dabs(fixed_acidity, volatile_acidity, citric_acid)\n",
"$$\n",
""""\n",
"\n",
"spark.sql(avg_acidity_udf_fn_dabs)\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "id-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"from pyspark.sql.functions import monotonically_increasing_id\n",
"\n",
"wine_df = wine_df.withColumn("wine_id", monotonically_increasing_id())\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "select-features-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"# Select engineered + original features (except target)\n",
"feature_columns = [\n",
"    "wine_id",\n",
"    "residual_sugar",\n",
"    "chlorides",\n",
"    "free_sulfur_dioxide",\n",
"    "total_sulfur_dioxide",\n",
"    "density",\n",
"    "PH",\n",
"    "sulphates",\n",
"    "alcohol",\n",
"    "log_residual_sugar",\n",
"    "log_chlorides",\n",
"    "sulphates_alcohol",\n",
"    "acidity_ratio",\n",
"    "PH_bin_low",\n",
"    "PH_bin_medium",\n",
"    "PH_bin_high"\n",
"]\n",
"\n",
"features_df = wine_df.select(*feature_columns)\n"
]
},
{
"cell_type": "code",
"execution_count": 0,
"metadata": {
"application/vnd.databricks.v1+cell": {
"cellMetadata": {},
"inputWidgets": {},
"nuid": "create-fe-table-cell",
"showTitle": false,
"tableResultSettingsMap": {},
"title": ""
}
},
"outputs": [],
"source": [
"# Create feature table with wine_id as primary key\n",
"from databricks.feature_engineering import FeatureEngineeringClient\n",
"\n",
"fe = FeatureEngineeringClient()\n",
"feature_table_name = "jpmc_group_catalog.mlops.wine_features_engineered_dabs"\n",
"\n",
"fe.create_table(\n",
"    name=feature_table_name,\n",
"    primary_keys=["wine_id"],\n",
"    df=features_df,\n",
"    schema=features_df.schema,\n",
"    description="Engineered features for wine quality Random Forest model"\n",
")\n"
]
},
